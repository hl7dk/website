{
    "website": {
      "style": "fhir.layout",
      "url": "http://hl7.dk/",
      "server": "apache",
      "org": "HL7 Denmark",
      "no-registry": "1",
      "layout": {
        "id": "hl7.fhir.dk.[code]",
        "canonical": "http://hl7.dk/fhir/[code]"
      }
    },
    "feeds": {
      "package": "package-feed.xml",
      "publication": "publication-feed.xml"
    },
    "layout-rules": [
      {
        "npm": "hl7.fhir.dk.*",
        "canonical": "http://hl7.dk/fhir/{4}",
        "destination": "/{4}"
      }
    ]
  }